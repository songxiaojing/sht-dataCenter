<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<!-- 系统中application 定义 -->
	<package name="application" namespace="/app" extends="struts-default,json-default">
		<global-results>
			<result name="error">/WEB-INF/jsp/common/exception.jsp</result>
			<result name="success">/index.html</result>
			<result name="message">/WEB-INF/jsp/common/message.jsp</result>
			<result name="lossUser">/login.jsp</result>
		</global-results>
		<action name="appView.*" class="ApplicationView" method="{1}">
			<result name="online">/index.html</result>
		</action>
	</package>
</struts>