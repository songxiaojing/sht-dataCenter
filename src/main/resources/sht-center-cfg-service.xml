<?xml version="1.0" encoding="UTF-8"?>
<application>
	<extendResource>
		<resource>tss-core-cfg-jdbc.properties</resource>
	</extendResource>
	<services>
		<NodeService>
			<applicationName>网安业务管理系统</applicationName>
			<applicationVersion>V0.9.0</applicationVersion>
			<deploymentArea>上海</deploymentArea>
			<license>1B2A6EA0B8CD8DD38EF61D332F665D98</license>
		</NodeService>
		<DirectoryStoreService>
			<fileStorageHomeDirectory>/home/hdfs/tss/run/storage/</fileStorageHomeDirectory>
			<temporaryStorageDirectory>/home/hdfs/tss/run/storage/temporary/</temporaryStorageDirectory>
			<ioBuffereSize>2048</ioBuffereSize>
		</DirectoryStoreService>
		<TssCoreMenuService>
			<tssCoreMenuFileName>tss-core-cfg-menu-tab-standard.xml</tssCoreMenuFileName>
		</TssCoreMenuService>
		<AuthenticationService>
			<jmxObjectName>topsec.tss.core:type=AuthenticationService</jmxObjectName>
			<uncheckResource>.png,.ico,.bmp,.jpg,.jpeg,.css,.js</uncheckResource>
			<uncheckServlets>/login.jsp,/authorize/authentication.login.action,/authorize/authorize.updateCurrentPassword.action,/authorize/authorize.updateUserPassword.action</uncheckServlets>
		</AuthenticationService>
		<ResourceModuleService>
			<columnFileName>tss-core-cfg-ssetopic-column.xml</columnFileName>
			<historyModuleClassList>HDelFormFeedback,HDelProcessingForm,HDelProcessing,HSseEventAttach,HSseEventDealForm,HSseEvent</historyModuleClassList>
		</ResourceModuleService>
		<EventProcessingEngineService>
			<dealEventSnCityName>上海市</dealEventSnCityName>
		</EventProcessingEngineService>
		<DataBaseStorageService>
			<driverClass>${jdbc.driverClassName}</driverClass>
			<jdbcUrl>${jdbc.url}</jdbcUrl>
			<user>${jdbc.username}</user>
			<password>${jdbc.password}</password>
			<initialPoolSize>20</initialPoolSize>
			<maxIdleTime>5</maxIdleTime>
			<maxPoolSize>20</maxPoolSize>
			<minPoolSize>5</minPoolSize>
		</DataBaseStorageService>
		<TssHdfsWebClientService>
			<tssHdfsWebApiImplClass>com.topsec.tss.hdfs.client.rest.RestTssClient</tssHdfsWebApiImplClass>
			<tssHdfsWebRootUrl>http://172.16.3.106:8080/tss-hdfs-web</tssHdfsWebRootUrl>
			<tssHdfsWebApiPath>/rest</tssHdfsWebApiPath>
		</TssHdfsWebClientService>
		<TssDMSService>
			<tssMDSWebHome>http://172.16.3.106:8080/tss-dms/</tssMDSWebHome>
			<tssMDSWebCacheMergeEvent>http://192.168.69.91:49080/tss-dms/mergeEventList.html</tssMDSWebCacheMergeEvent>
			<tssMDSWebCacheIp>http://172.16.3.106:8080/tss-dms/ipList.html</tssMDSWebCacheIp>
			<importIpSegmentsPath>http://172.16.3.106:8080/tss-dms/resource/ipSegments/import</importIpSegmentsPath>
		</TssDMSService>
	</services>
</application>	